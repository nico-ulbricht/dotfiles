# navigation
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias .....="cd ../../../.."

# tooling
alias cp="cp -v"
alias mv="mv -v"
alias rm="rm -v"
alias where="which"

# git
alias g="git"
alias master="git checkout master"

# k8s  
alias kc="kubectl"
alias kcd="kubectl delete"
alias kcdd="kubectl delete deploy"
alias kcdn="kubectl delete namespace"
alias kcdp="kubectl delete pods"
alias kcds="kubectl delete svc"
alias kcg="kubectl get"
alias kcgd="kubectl get deploy"
alias kcgn="kubectl get namespaces"
alias kcgp="kubectl get pods"
alias kcgs="kubectl get svc"

# yarn
alias y="yarn"
alias yb="yarn build"
alias ybw="yarn build:watch"
alias ys="yarn start"
alias yt="yarn test"
alias ytw="yarn test:watch"